<Page x:Class="Polimer.MyPages.YchetPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Polimer.MyPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Страница учета">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
            <Button x:Name="BPostHaYch" Content="Поставка на учет" Style="{StaticResource ModernButton}" Click="BPostHaYch_Click"/>
            <Button x:Name="BCpican" Content="Списание" Style="{StaticResource ModernButton}" Click="BCpican_Click"/>
        </StackPanel>
    </Grid>
</Page>
